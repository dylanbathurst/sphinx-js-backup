{
  "manifest_version": 3,
  "name": "Sphinx Browser Extension",
  "version": "0.0.1",
  "description": "Decentralize social media, and take back your data.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "static/icon.png",
      "48": "static/icon.png",
      "128": "static/icon.png"
    }
  },
  "icons": {
    "16": "static/icon.png",
    "48": "static/icon.png",
    "128": "static/icon.png"
  },
  "permissions": ["activeTab"]
}
